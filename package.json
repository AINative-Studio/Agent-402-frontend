{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit -p tsconfig.app.json && vite build",
    "build:dev": "tsc --noEmit -p tsconfig.app.json && vite build --mode development",
    "build:staging": "tsc --noEmit -p tsconfig.app.json && vite build --mode staging",
    "build:prod": "tsc --noEmit -p tsconfig.app.json && vite build --mode production",
    "build:analyze": "npm run build:prod && open dist/stats.html",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ainative/ai-kit": "file:../ai-kit/packages/react",
    "@tanstack/react-query": "^5.62.11",
    "@types/elliptic": "^6.4.18",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "elliptic": "^6.6.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.57.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
